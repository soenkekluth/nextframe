{
  "name": "nextframe",
  "version": "1.6.1",
  "description": "Promise for requestAnimationFrame, plus delayed and sequenced frames",
  "amdName": "nextframe",
  "module": "es/nextframe.js",
  "jsnext:main": "es/nextframe.js",
  "main": "lib/nextframe.js",
  "browser": "dist/nextframe.js",
  "scripts": {
    "test": "ava --verbose",
    "pretest": "yarn build",
    "build": "yarn build:cjs && yarn build:es && yarn build:browser",
    "example": "node example.js",
    "build:es": "cross-env NODE_ENV=production FORMAT=es rollup -c",
    "build:umd": "cross-env NODE_ENV=production FORMAT=umd rollup -c",
    "build:cjs": "cross-env NODE_ENV=production FORMAT=cjs rollup -c",
    "build:browser": "cross-env NODE_ENV=production FORMAT=iife rollup -c",
    "dev": "rollup -c --watch",
    "reload": "livereload 'build/'",
    "watch": "cross-env FORMAT=iife NODE_ENV=development run-p reload dev",
    "prepublish": "yarn test"
  },
  "files": [
    "lib",
    "src",
    "es"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/soenkekluth/nextframe.git"
  },
  "keywords": [
    "requestAnimationFrame",
    "Promise",
    "resolve",
    "delay",
    "frame",
    "next frame",
    "sequence",
    "performance",
    "jank",
    "jankfree",
    "jank free",
    "animation"
  ],
  "author": "SÃ¶nke Kluth <soenke.kluth@gmail.com> (http://soenkekluth.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/soenkekluth/nextframe/issues"
  },
  "homepage": "https://github.com/soenkekluth/nextframe#readme",
  "dependencies": {
    "raf": "^3.3.2"
  },
  "peerDependencies": {
    "raf": ">= 3.3.2"
  },
  "devDependencies": {
    "ava": "0.19.1",
    "babel-core": "6.25.0",
    "babel-plugin-external-helpers": "6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "cross-env": "5.0.1",
    "performance-now": "2.1.0",
    "rollup": "0.43.0",
    "rollup-plugin-babel": "2.7.1",
    "rollup-plugin-commonjs": "8.0.2",
    "rollup-plugin-eslint": "3.0.0",
    "rollup-plugin-memory": "2.0.0",
    "rollup-plugin-node-resolve": "3.0.0",
    "rollup-plugin-replace": "1.1.1",
    "rollup-plugin-uglify": "2.0.1",
    "rollup-watch": "4.0.0"
  }
}
